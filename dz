from math import sqrt

# а
x = int(input())
print(-1 / x ** 2)

# б
a = int(input())
b = int(input())
c = int(input())
print(a / (b*c))

# в
print((a/b)*c)

# г
print((a+b)/2)

# д
print(5,45 * ((a+2*b)/(2-a)))

# е
print((-b + sqrt(b**2 - 4*a*c)) / 2*a)

# ж
print((-b + 1/a) / (2/c))

# з
print(1 / (1 + ((a + b) / 2)))

# и
print( 1 / (1 + (1 / (2 + (1 / (2 + (3/5)))))))

# к
m = int(input())
n = int(input())
print(2 ** m*n)
